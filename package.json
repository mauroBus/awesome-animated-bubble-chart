{
  "name": "awesome-animated-bubble-chart",
  "version": "1.0.0",
  "description": "Animated Bubble Chart built with React, Redux and TypeScript",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 edge versions",
    "last 2 ios versions",
    "last 2 safari versions",
    "last 2 opera versions",
    "last 2 ChromeAndroid versions",
    "ie >= 11"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack --config=webpack/config-prod.js",
    "clean": "rimraf build",
    "deploy": "gh-pages -d dist",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "extractlocales": "NODE_ENV=locale format-message extract -g underscored_crc32 -o src/locales/en.json 'src/**/*.js' 'src/**/*.ts' 'src/**/*.tsx'",
    "lint": "yarn eslint && yarn stylelint && yarn tsc",
    "prebuild": "rimraf build",
    "prettier": "prettier --write \"**/*.{js,ts,tsx}\"",
    "serve": "node ./json-server.js",
    "start": "webpack-dev-server --config=webpack/config-dev-server.js",
    "stylelint": "stylelint \"src/**/*.css\"",
    "test": "TZ=UTC jest --config jest/test-config.js",
    "test-watch": "TZ=UTC jest --config jest/base-config.js --watch",
    "tsc": "tsc"
  },
  "engines": {
    "node": ">=10.16"
  },
  "devDependencies": {
